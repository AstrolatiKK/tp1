var itogBalance;

$(document).ready(function()
{
   var clipboard = new Clipboard('#PayeerAccount');
   clipboard.on('success', function(e)
   {
      $(e.trigger).addClass('copy-payeer-account');
      /*
      var PayeerAccountN = $(e.trigger).parent('li').find('b');
      PayeerAccountN.addClass('copy-payeer-account');

      setTimeout(function()
      {
         PayeerAccountN.removeClass('copy-payeer-account');
      }, 1000);
      */
   });

   $('.header-overlay .balance-item').hover(
   function()
   {
      $(this).css({'background': $(this).css('border-top-color')});
   },
   function()
   {
      $(this).css({'background': ''});
   });

   
   $('.wallet').on('mouseenter click', '.fa-info-circle', function ()
   {
      var $thisOffset = $(this).offset();

      var tooltip = $('.wallet .js-tool-tip-ordertype');
      tooltip.html($(this).data('title'));
      tooltip.show();

      tooltip.offset({
         top: $thisOffset.top + 20,
         left: $thisOffset.left - tooltip.outerWidth() / 2 + 2
      });
   });

   $('.wallet .fa-info-circle').on('mouseleave', function ()
   {
      $('.wallet .js-tool-tip-ordertype').hide();
   });
});

function balanceUpdate()
{
	ajaxLoad({id: '#', url: '/bitrix/components/payeer/account.info2/templates/top2/ajax.php?action=balance2&sessid=f4b3af2df9e4e7e7cb30490755c7a644', afterSuccess: 'afterBalanceUpdate'}, {nprogress: 'N', type: 'null', formType: 'json'});
}

function afterBalanceUpdate(e, data)
{
   //pre(['afterBalanceUpdate', data]);

   if (data != '')
	{
      itogBalance = data;

      var itogSum = 0;

      if (data.balance != '' && data.balance != undefined)
      {
         for (var curr in data.balance)
         {
            //pre([curr, data.balance[curr]]);

            var ar = data.balance[curr];

            var n = Number(ar[0].replace(/[\s,]+/g, '') + '.' + ar[1]);

            if (isNaN(n))
            {
                n = 0;
            }

            totalByCurrency = Math.ceil10(arInternalExchangeRate[curr + '/' + itogCurr] * n, -arCurrPr[itogCurr]);

            if (isNaN(totalByCurrency))
            {
                totalByCurrency = 0;
            }

            itogSum += totalByCurrency;
            //pre(itogSum);

            $('.account_balance .curr-' + curr + ' span.int').html(ar['0']);
            $('.account_balance .curr-' + curr + ' span.pr').html('.' + ar['1']);

            $('.header .wallet .balance .balance-curr--' + curr.toLowerCase() + ' s').html(ar['0'] + '<sup>.' + ar['1'] +'</sup>');

            $('.div-balance-update .balance-item--' + curr.toLowerCase() + ' .n-select__selected-value-caption').html(ar['0'] + '.' + ar['1']);
         }

         if ($('.trade-management').length)
         {
            var arPair = $('#id_trade_pair').val().split('_');
            //pre(data.balance[arPair[0]]);

            var sumForSell = data.balance[arPair[0]].join('.').replace(/[^0-9\.]+/g, '');
            var sumForBuy = data.balance[arPair[1]].join('.').replace(/[^0-9\.]+/g, '');

            $('#id_trade_balance_for_sell').val(sumForSell);
            $('#id_trade_balance_for_sell').closest('.trade-management__balance-sum').find('div').html(tradeAmountFormat(sumForSell));

            $('#id_trade_balance_for_buy').val(sumForBuy);
            $('#id_trade_balance_for_buy').closest('.trade-management__balance-sum').find('div').html(tradeAmountFormat(sumForBuy));
         }
      }

      if (data.balancet != '' && data.balancet != undefined)
      {
         var bShowActiveOrdersTitle = false;

         for (var curr in data.balancet)
         {
            var ar = data.balancet[curr];
            //pre([curr, ar]);

            var n = Number(ar[0].replace(/[\s,]+/g, '') + '.' + ar[1]);

            if (isNaN(n))
            {
                n = 0;
            }

            totalByCurrency = Math.ceil10(arInternalExchangeRate[curr + '/' + itogCurr] * n, -arCurrPr[itogCurr]);

            if (isNaN(totalByCurrency))
            {
                totalByCurrency = 0;
            }

            itogSum += totalByCurrency;

            $('.account_balance .trade-curr-' + curr + ' span.int').html(ar['0']);
            $('.account_balance .trade-curr-' + curr + ' span.pr').html('.' + ar['1']);

            $('.header .wallet .balance .balance-trade-curr--' + curr.toLowerCase() + ' s').html(ar['0'] + '<sup>.' + ar['1'] +'</sup>');

            if (n > 0)
            {
               bShowActiveOrdersTitle = true;

               $('.header .wallet .balance .balance-trade-curr--' + curr.toLowerCase()).show();

               $('.account_balance .trade-curr-' + curr).removeClass('balance-0');

               if ($('.abn-h2 span.active').data('type') == 'active-orders')
               {
                  $('.account_balance .trade-curr-' + curr).show();
               }
            }
            else
            {
               $('.header .wallet .balance .balance-trade-curr--' + curr.toLowerCase()).hide();

               $('.account_balance .trade-curr-' + curr).removeClass('balance-0').addClass('balance-0');

               if ($('.abn-h2 span.active').data('type') == 'active-orders')
               {
                  $('.account_balance .trade-curr-' + curr).hide();
               }
            }
         }

         if (bShowActiveOrdersTitle)
         {
            $('.balance-title-active-orders').show();
         }
         else
         {
            $('.balance-title-active-orders').hide();

            if ($('.main_abalance').length)
            {
               $('.account_balance_new > li').hide();
         		$('.account_balance_new > li.all').not('.balance-0').show();

         		$('.abn-h2 span.active').removeClass('active');
         		$('.main_abalance span[data-type="all"]').addClass('active');
            }
         }
      }

      
      itogSum = number_format(itogSum, arCurrPr[itogCurr], '.', ',');
      //pre('itogSum = ' + itogSum);

      arItog = itogSum.split('.');

      $('.account_balance .itog span.int').html(arItog['0']);
      $('.account_balance .itog span.pr').html('.' + arItog['1']);

      $('.account_balance .itog').attr('class', '').addClass('itog itog-' + itogCurr);
      $('.account_balance li.itog .val .curr').attr('class', '').addClass('curr fa fa-' + itogCurr.toLowerCase());

      $('.header-total .icon-padding-top').attr('class', '').addClass('icon-padding-top icon-' + itogCurr.toLowerCase());
      $('.header-total .btn--amount s').html(arItog['0'] + '<sup>.' + arItog['1'] + '</sup>');

      $('.wallet-total .icon-padding-top').attr('class', '').addClass('icon-padding-top icon-' + itogCurr.toLowerCase());
      $('.wallet .wallet-total s').html(arItog['0'] + '<sup>.' + arItog['1'] + '</sup>');

      	}
	else
	{
		window.location.reload();
	}
}

var iBalanceUpdate = setInterval(function()
{
	balanceUpdate();
}, 60000 * 5);

function onAfterBalanceUpdate(data)
{
   //pre(['onAfterBalanceUpdate', data]);

   afterBalanceUpdate('#', data);
}

function onAfterCardBalanceUpdate(data)
{
   //pre(['onAfterCardBalanceUpdate', data]);

   afterBalanceUpdate('#', {cards: [data.card]});
}

function setTypeLight(type)
{
   $('#chart').html('');

	$('body').removeClass('black night');

   $('.actions-theme a').removeClass('selected');
   $('.actions-theme a.' + type).addClass('selected');

	switch (type)
	{
		case 'light':
		break;

		case 'contrast':
			$('body').addClass('black night');
		break;
	}

   if (window['chartCreate']) chartCreate();
   $('.chart-timeline button.current').click();
   $('.trade-management__times button.is-active-time-btn').click();

	ajaxLoad({id: '#', url: '/bitrix/components/payeer/account.info2/templates/top2/ajax.php?action=param&param=typelight&typelight=' + type + '&sessid=f4b3af2df9e4e7e7cb30490755c7a644'}, {nprogress: 'N', type: 'null'});
}

function afterCardsCheck(e, data)
{
   $.pjax(
   {
      url: data,
      type: 'GET',
      container: '#pjax-container'
   });
}


function tradeAfterRequestRefundByCurrency(e, data)
{
   //pre(data['winData']);

   $('.dialog-trade-request-detail').remove();

   $('.content').addClass('blur');

   $('.header-overlay .statusinfo .statusinfo-icon > span').attr('class', '').addClass('icon-approved');

   $('.header-overlay .statusinfo .statusinfo-title').html(data['winData']['title']);
   $('.header-overlay .statusinfo .statusinfo-direction').html(data['winData']['direction']);
   $('.header-overlay .statusinfo .statusinfo-descr').html(data['winData']['descr']);

   $('.header-overlay').addClass('show').find('.statusinfo').show();

   winErrorAutoClose();
}


setTypeLight('contrast');
</script>


