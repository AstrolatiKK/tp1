<script type="text/javascript">
var defItogCurr = 'USD';

$(document).ready(function()
{
	balanceUpdate();

	$('.abn-h2 span').click(function()
	{
				let type = $(this).data('type');

		
		$('.account_balance_new > li').hide();
		$('.account_balance_new > li.'+type).not('.balance-0').show();

		$('.abn-h2 span.active').removeClass('active');
		$(this).addClass('active');
	});

	$(document).keyup(function(e)
	{
		//alt + r
		if (e.altKey && e.keyCode === 82) {
			localStorage.removeItem("sortBalance");
			location.reload();
		}
	});

	if(!isMobile(navigator.userAgent)) {
		new Sortable(main_list_balance_block, {
			swap: true,
			swapClass: 'highlight',
			animation: 1000,
			store: {
				get: function (sortable) {
					var order = localStorage.getItem("sortBalance");
					$( "#main_list_balance_block" ).removeClass('hide');
					return order ? order.split(',') : [];
				},
				set: function (evt) {
					localStorage.setItem("sortBalance", evt.toArray());
				}
			}
		});
		$( "#main_list_balance_block" ).disableSelection();
	} else {
		$( "#main_list_balance_block" ).removeClass('hide');
	}
});


function afterHideWallet(e, response)
{
	$('.wtop .balance li.' + e.curr).hide();
}


function afterShowHiddenWallets(e, response)
{
	$('.wtop .balance li').show();
}
</script>
</div>
			</div>
		</div>
	</main>
</div>
<div class="clear"></div>
</body>
</html>