<script type="text/javascript">
	try
	{
		var socketNewTrade = io('https://wss.payeer.com?UserID=32840136&auth_hash=1fnjn4r4pkq9ce5u6lksktfv654dgkb8jp9pvhdthmg26ptafh38se1ec7rgaoe38aajaftk2qc0fod01g1h1gs8j8stitc7achf233',
		{
		  transports: ['websocket']
		});
		socketNewTrade.onceEvent = socketNewTrade.onceEvent2 = true;
		socketNewTrade.inProgress = false;
	   socketNewTrade.countM = 0

		socketNewTrade.on('tickets', function (data)
		{
			$('#btn-support span').html(data.new);

			if (data.new > 0)
			{
				$('#btn-support span').removeClass('empty');
			}
			else
			{
				$('#btn-support span').addClass('empty');
			}
		});

		socketNewTrade.on('ws', function (data)
		{
			if (window[data.handler] != undefined)
			{
				window[data.handler](data);
			}
		});

		socketNewTrade.on('balance', function (data)
		{
			console.log('ws balance', data);
		});

	   socketNewTrade.on('toall', function (data)
	   {
	      if (window[data.handler] != undefined)
	      {
	         window[data.handler](data);
	      }
	   });

		socketNewTrade.on('sendtoall', function (data)
	   {
	      if (window[data.handler] != undefined)
	      {
	         window[data.handler](data);
	      }
	   });
	}
	catch(e)
	{
	  pre(e.message);
	}
</script>
